    "react-query-params": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "semantic-ui-react": "^0.77.2",
    "slugify": "^1.3.0",
    "spark-md5": "^3.0.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
