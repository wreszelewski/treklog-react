    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "bluebird": "^3.5.5",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "cesium": "^1.59.0",
    "chalk": "1.1.3",
